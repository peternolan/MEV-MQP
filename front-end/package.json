{
  "name": "mev-mqp-front-end",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.13"
  },
  "dependencies": {
    "@devexpress/dx-grid-core": "^1.10.1",
    "@devexpress/dx-react-core": "^1.10.1",
    "@devexpress/dx-react-grid": "^1.10.1",
    "@devexpress/dx-react-grid-material-ui": "^1.10.1",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "4.2.2"
  },
  "scripts": {
    "startLocal": "npm run env:devLocal && react-scripts start",
    "start": "npm run env:devRemote && react-scripts start",
    "build": "npm run env:deploy && react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js --ext .jsx src test",
    "env:devLocal": "cp environments/devLocal.env .env",
    "env:devRemote": "cp environments/devRemote.env .env",
    "env:deploy": "cp environments/deploy.env .env",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "react-scripts test --env=jsdom --reporters=jest-junit --reporters=default --ci --coverage --coverageDirectory=coverage",
    "coverage": "react-scripts test --env=jsdom --coverage"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/build/**"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "coverage/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
